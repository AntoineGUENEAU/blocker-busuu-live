{
    "manifest_version": 3,
    "name": "Blocker busuu live",
    "description": "Chrome extension to hide promotional banners",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": [
                "https://www.busuu.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}
